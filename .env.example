# API Authentication
API_KEY=your_api_key_here

# Logging
LOG_LEVEL=INFO

# LLM API Keys
LITELLM_API_KEY=your_openai_api_key_here
GOOGLE_API_KEY=your_gemini_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# LLM Configuration
LITELLM_MODEL_DEFAULT_TEMPERATURE=0.7

# Database Configuration (these are used by judge-service)
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=panopticon
DATABASE_URL=postgresql://postgres:postgres@postgres:5432/panopticon

# Service URLs (used by main-app to communicate with services)
QUERY_STORAGE_URL=http://query-storage:8000
EVALUATION_STORAGE_URL=http://evaluation-storage:8000
JUDGE_SERVICE_URL=http://judge-service:8000
